<head>
	<meta charset="UTF-8" />
	<title><%= meta.title %></title>
	<meta name="description" content="<%= meta.description %>" />
	<meta name="keywords" content="<%= meta.keywords %>" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<meta name="robots" content="<%= meta.robots %>" />
	<meta name="author" content="Vite Template" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<link rel="canonical" href="<%= meta.url %>" />
	<meta name="theme-color" content="#040413" />

	<!-- OpenGraph -->
	<meta property="og:url" content="<%= meta.url %>" />
	<meta property="og:title" content="<%= meta.title %>" />
	<meta property="og:description" content="<%= meta.description %>" />
	<meta property="og:type" content="website" />
	<meta
		property="og:image"
		content="<%= meta.VITE_SITE_URL %>/dist/img/og_image.webp"
	/>
	<meta property="og:site_name" content="Vite Template" />
	<meta property="og:locale" content="pl_PL" />
	<meta property="og:image:width" content="1200" />
	<meta property="og:image:height" content="630" />
	<meta
		property="og:image:alt"
		content="Logotyp Vite Template, na jasnym tle."
	/>

	<!-- Microsoft Tiles -->
	<meta name="msapplication-TileColor" content="#f9f9f9" />
	<meta
		name="msapplication-TileImage"
		content="<%= meta.VITE_SITE_URL %>/dist/icons/mstile-144x144.png"
	/>
	<meta
		name="msapplication-config"
		content="<%= meta.VITE_SITE_URL %>/browserconfig.xml"
	/>

	<!-- Application -->
	<meta name="application-name" content="Vite Template" />

	<!-- Twitter -->
	<meta name="twitter:card" content="summary_large_image" />
	<meta property="twitter:domain" content="<%= meta.VITE_SITE_URL %>" />
	<meta property="twitter:url" content="<%= meta.url %>" />
	<meta name="twitter:title" content="<%= meta.title %>" />
	<meta name="twitter:description" content="<%= meta.description %>" />
	<meta
		name="twitter:image"
		content="<%= meta.VITE_SITE_URL %>/dist/img/og_image.webp"
	/>

	<!-- Apple Web App -->
	<meta name="apple-mobile-web-app-title" content="Vite Template" />
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<meta name="apple-mobile-web-app-status-bar-style" content="black" />
	<meta name="mobile-web-app-capable" content="yes" />
	<meta name="format-detection" content="telephone=no" />
	<meta name="google" content="notranslate" />

	<!-- Styles -->
	<link rel="stylesheet" href="<%= meta.cssUrl %>" />

	<!-- Favicon & Icons -->
	<link rel="icon" type="image/x-icon" href="<%= meta.url %>favicon.ico" />
	<link
		rel="icon"
		type="image/png"
		sizes="16x16"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/favicon-16x16.png"
	/>
	<link
		rel="icon"
		type="image/png"
		sizes="32x32"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/favicon-32x32.png"
	/>
	<link
		rel="icon"
		type="image/png"
		sizes="48x48"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/favicon-48x48.png"
	/>

	<!-- Apple Touch Icons -->
	<link
		rel="apple-touch-icon"
		sizes="57x57"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-57x57.png"
	/>
	<link
		rel="apple-touch-icon"
		sizes="60x60"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-60x60.png"
	/>
	<link
		rel="apple-touch-icon"
		sizes="72x72"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-72x72.png"
	/>
	<link
		rel="apple-touch-icon"
		sizes="76x76"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-76x76.png"
	/>
	<link
		rel="apple-touch-icon"
		sizes="114x114"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-114x114.png"
	/>
	<link
		rel="apple-touch-icon"
		sizes="120x120"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-120x120.png"
	/>
	<link
		rel="apple-touch-icon"
		sizes="144x144"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-144x144.png"
	/>
	<link
		rel="apple-touch-icon"
		sizes="152x152"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-152x152.png"
	/>
	<link
		rel="apple-touch-icon"
		sizes="167x167"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-167x167.png"
	/>
	<link
		rel="apple-touch-icon"
		sizes="180x180"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-180x180.png"
	/>
	<link
		rel="apple-touch-icon"
		sizes="1024x1024"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-icon-1024x1024.png"
	/>

	<!-- Apple Startup Images -->
	<link
		rel="apple-touch-startup-image"
		media="(device-width: 320px) and (device-height: 568px) and (-webkit-device-pixel-ratio: 2) and (orientation: portrait)"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-startup-image-640x1136.png"
	/>
	<link
		rel="apple-touch-startup-image"
		media="(device-width: 320px) and (device-height: 568px) and (-webkit-device-pixel-ratio: 2) and (orientation: landscape)"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-startup-image-1136x640.png"
	/>
	<link
		rel="apple-touch-startup-image"
		media="(device-width: 375px) and (device-height: 667px) and (-webkit-device-pixel-ratio: 2) and (orientation: portrait)"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-startup-image-750x1334.png"
	/>
	<link
		rel="apple-touch-startup-image"
		media="(device-width: 375px) and (device-height: 667px) and (-webkit-device-pixel-ratio: 2) and (orientation: landscape)"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-startup-image-1334x750.png"
	/>
	<link
		rel="apple-touch-startup-image"
		media="(device-width: 375px) and (device-height: 812px) and (-webkit-device-pixel-ratio: 3) and (orientation: portrait)"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-startup-image-1125x2436.png"
	/>
	<link
		rel="apple-touch-startup-image"
		media="(device-width: 375px) and (device-height: 812px) and (-webkit-device-pixel-ratio: 3) and (orientation: landscape)"
		href="<%= meta.VITE_SITE_URL %>/dist/icons/apple-touch-startup-image-2436x1125.png"
	/>
	<!-- (kolejne media queries analogicznie...) -->

	<!-- Yandex Browser Widget -->
	<link
		rel="yandex-tableau-widget"
		href="<%= meta.VITE_SITE_URL %>/yandex-browser-manifest.json"
	/>

	<!-- fonts -->

	<link rel="preconnect" href="https://fonts.googleapis.com" />
	<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
	<link
		href="https://fonts.googleapis.com/css2?family=Montserrat:wght@600&family=Open+Sans:ital,wght@0,300..800;1,300..800&display=swap"
		rel="stylesheet"
	/>

	<!-- JSON-LD Schema.org -->
	<script type="application/ld+json">
		{
			"@context": "https://schema.org",
			"@type": "Organization",
			"name": "Vite Template – Szablon Multipage",
			"legalName": "Vite Template",
			"description": "<%= meta.description %>",
			"url": "<%= meta.url %>",
			"logo": "<%= meta.VITE_SITE_URL %>/dist/img/logo.png",
			"image": "<%= meta.VITE_SITE_URL %>/dist/img/logo.png"
			/* Dodaj tu inne statyczne dane, jeśli potrzebujesz: kontakt, adres itp. */
		}
	</script>
</head>
